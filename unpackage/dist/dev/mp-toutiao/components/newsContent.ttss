
.refresh-part-load {
	width: 100%;
	height: 50px;
	display: none;
	transition: all 1s ease-in-out;
}
.refresh-part-not-load {
	width: 0;
	height: 0;
}
.scroll-view_V {
	/* 文本不会换行，文本会在在同一行上继续，直到遇到 <br> 标签为止。 */
	width: 96vw;
	height: 100%;
	margin: 0 2vw 0 2vw;
}
.scroll-view-item_V {
	width: 100%;
}

